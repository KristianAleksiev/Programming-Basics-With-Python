daily_money = float(input())
daily_profit = float(input())
expenses = float(input())
gift_price = float(input())
total_money = (5 * daily_money) + (5 * daily_profit) - expenses
diff = abs(total_money - gift_price)
if total_money >= gift_price:
    print(f"Profit: {total_money:.2f} BGN, the gift has been purchased.")
else:
    print(f"Insufficient money: {diff:.2f} BGN.")
